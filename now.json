{
	"name": "NodeLabs-KPI-schedule-bot",
	"version": 2 ,
	"builds": [{ "src": "parser.js", "use": "@now/node", "config": { "maxLambdaSize": "45mb" }}],
	"build": {
		"env": {
			"BOT_TOKEN": "@bot_token"
		}
	},
	"routes": [{
		"headers": {
			"Access-Control-Allow-Origin": "*",
			"Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
			"Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
		},
		"src": "/.*",
		"dest": "/parser.js"
	}]
}